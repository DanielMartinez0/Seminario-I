\mode<presentation>
\definecolor{dark green}{rgb}{0.255,0.678,.286}
\definecolor{light green}{rgb}{.024,0.604,.494}
% Settings
\setbeamercolor*{title page header}{fg=dark green}
\setbeamercolor*{author}{fg=black}
\setbeamercolor*{date}{fg=black}

\useinnertheme{rectangles}
\setbeamercolor{section in head/foot}{bg=dark green, fg=white}
\setbeamercolor{subsection in head/foot}{bg=light green, fg=white}
\setbeamercolor{section in toc}{fg=dark green}
\setbeamercolor{subsection in toc}{fg=black}
\setbeamercolor{block title}{fg=white,bg=dark green}
\setbeamercolor{block body}{fg=black,bg=white}

\setbeamercolor{enumerate item}{bg= dark green,fg=white}

\setbeamercolor*{item}{fg=dark green}

% Items
\setbeamertemplate{items}[square]
\setbeamertemplate{sections/subsections in toc}[square]

\mode
<all>
